<h1>Articles#edit</h1>
<p>Find me in app/views/articles/edit.html.erb</p>

<%= simple_form_for @article do |f| %>

  <%= f.input :title, class: "form_control" %>
  <%= f.input :content, class: "form_control" %>
  <%= f.label :picture %>
  <%= f.file_field :picture %>

  <%= f.button :submit, class: "btn-primary", id:"input_validate" %>

<% end %>

